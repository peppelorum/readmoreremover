{
    "name": "Läs också-raderaren",
    "version": "0.1.1",
    "manifest_version": 2,
    "description": "Raderar alla förekomster av 'Läs också' som finns på svd.se",
    "content_scripts": [
        {
            "matches": ["*://*.svd.se/*"],
            "js": ["script.js"]
        }
    ],
    "permissions": [
        "*://*.svd.se/*"
    ],
    "web_accessible_resources": [
        "badge.png",
        "x.png"
    ],
    "icons": { "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
